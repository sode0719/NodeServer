extends layout

block title
  title= title

block css
  link(rel='stylesheet', href='http://172.16.1.12:3000/css/dispatcher.css')

block content
  .container
    if login
      #js-id(style='display: none')= id
      if delegate
        #dispatcher
      else
        .alert.alert-warning 代表者のみ利用できます
    else
      .alert.alert-danger ログインしてください

block script
  script(src='https://npmcdn.com/react@15/dist/react-with-addons.js')
  script(src='https://unpkg.com/react-dom@15/dist/react-dom.js')
  script(src='https://npmcdn.com/reactstrap@2/dist/reactstrap.min.js')
  if login
    script(src='https://code.jquery.com/ui/1.12.1/jquery-ui.js')
    script(src="http://172.16.1.12:3000/js/component/dispatcher.js")
